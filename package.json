  "scripts": {
    "build": "ember build; npm run critical-css",
    "build-production": "ember build -prod; npm run critical-css",
    "critical-css": "cat dist/index.html | critical --base dist/ --inline --minify > index.tmp && mv index.tmp dist/index.html",
    "deploy-firebase-staging": "firebase use staging; firebase deploy --only hosting",
    "deploy-firebase-production": "firebase use production; firebase deploy --only hosting",
    "start": "ember server",
    "test": "ember test"
  },
